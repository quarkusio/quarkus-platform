      <activation>
        <property>
          <name>env.CI</name>
        </property>
      </activation>
      <build>
        <pluginManagement>
          <plugins>
            <plugin>
              <artifactId>maven-gpg-plugin</artifactId>
              <configuration>
                <bestPractices>true</bestPractices>
                <useAgent>false</useAgent>
                <signer>bc</signer>
              </configuration>
            </plugin>
          </plugins>
        </pluginManagement>
        <plugins>
