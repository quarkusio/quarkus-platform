<settings>
  <profiles>
    <profile>
      <id>google-mirror</id>
      <repositories>
        <repository>
          <id>google-maven-central</id>
          <name>GCS Maven Central mirror</name>
          <url>https://maven-central.storage-download.googleapis.com/maven2/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <snapshots>
              <enabled>false</enabled>
          </snapshots>
          <id>kogito</id>
          <name>kogito</name>
          <url>https://repository.jboss.org/nexus/content/groups/kogito-public/</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>google-maven-central</id>
          <name>GCS Maven Central mirror</name>
          <url>https://maven-central.storage-download.googleapis.com/maven2/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>google-mirror</activeProfile>
  </activeProfiles>
</settings>
